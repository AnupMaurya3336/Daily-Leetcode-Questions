// 1653. Minimum Deletions to Make String Balanced


class Solution {
    public int minimumDeletions(String s) {int ans=0;int b=0;
    for(int i=0;i<s.length();i++){
        if(s.charAt(i)=='b'){
            b++;
        }else if(b>0){
            ans++;
            b--;
        }
    }
    return ans;    
    }
}

