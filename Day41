// 2154. Keep Multiplying Found Values by Two

class Solution {
    public int findFinalValue(int[] nums, int original) {
        while(find(nums,original)){
            original = 2 * original;
        }
        return original;
    
        
    }
    public static boolean find(int [] arr ,int target){
        for(int i=0;i<arr.length;i++){
            if(arr[i]==target){
                return true;
            }
        }
        return false;

    }
}
