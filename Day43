// 905. Sort Array By Parity

class Solution {
    public int[] sortArrayByParity(int[] nums) {
        int [] arr1=new int[nums.length];
        int index=0;

        for(int i=0;i<nums.length;i++){
            if(nums[i]%2==0){
                arr1[index]=nums[i];
                index++;

â€¦
        return arr1;
        
    }
}
