// 268. Missing Number


class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        // int sum=0;
        // for(int i=0;i<n;i++){
        //     sum+=nums[i];
        // }
        // int ans=0;
        // for(int i=0;i<=n;i++){
        //     ans+=i;
        // }
        // return ans-sum;

         int xor=n;
         for(int i=0;i<n;i++){
            xor^=i;
            xor^=nums[i];
         }
         return xor;
    }
}
