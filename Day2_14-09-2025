//  26. Remove Duplicates from Sorted Array
//  Input: nums = [1,1,2]
//  Output: 2, nums = [1,2,_]


class Solution {
    public int removeDuplicates(int[] nums) {
        int n=nums.length;
        int idx=1;
        for(int i=1;i<n;i++){
            if(nums[i]!=nums[i-1]){
                nums[idx++]=nums[i];
            }
        }
        return idx;
    }
}
