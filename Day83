//  3238. Find the Number of Winning Players

class Solution {
    public int winningPlayerCount(int n, int[][] pick) {
        Map<Integer, Map<Integer, Integer>> map = new HashMap<>();

        for (int[] p : pick) { //p name se ak array banaya hai
            int player = p[0]; //p[0] ko player bataya hai
            int color = p[1]; // p[1] ko color bataya hai
            if (!map.containsKey(player)) {
                map.put(player, new HashMap<>());
            }
            Map<Integer, Integer> plcolor = map.get(player);
            plcolor.put(color, plcolor.getOrDefault(color, 0) + 1);
        }
        int winner = 0;
        for (int i = 0; i < n; i++) {
            boolean won = false;
            Map<Integer, Integer> plcolor = map.getOrDefault(i,new HashMap<>());
            for(int count : plcolor.values()){
                if(count > i){
                    won =true;
                    break;
                }
            }
            if(won){
                winner++;
            }

        }
        return winner;

    }
}
