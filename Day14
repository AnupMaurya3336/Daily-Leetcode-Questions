// 387. First Unique Character in a String
class Solution {
    public int firstUniqChar(String s) {
        int l=s.length();
        for(int i=0;i<l;i++){
            boolean flag = false;
            for(int j=0;j<l;j++){
                if( i != j && s.charAt(i) == s.charAt(j)){
                    flag = true;
                    break;
                }  
            }
             if( !flag) return i;
        }
        return -1;
        
    }
}
