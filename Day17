//   3688. Bitwise OR of Even Numbers in an Array
class Solution {
    public int evenNumberBitwiseORs(int[] nums) {
        int sum = 0;
        int l=nums.length;
        for (int i = 0; i < l; i++) {
            if (nums[i] % 2 == 0) {
                sum = sum | nums[i];
            }
           
        }
        return sum;
    }
}
